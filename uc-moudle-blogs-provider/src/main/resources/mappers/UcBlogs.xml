<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.uc.moudle.blogs.provider.mapper.BlogsMapper">        <resultMap id="baseResultMap" type="com.uc.blogs.api.entity.UcBlogs">        <id column="id" property="id" jdbcType="INTEGER"/>        <result column="blog_content" property="blogContent" jdbcType="VARCHAR"/>        <result column="blog_belong_to" property="blogBelongTo" jdbcType="INTEGER"/>        <result column="blog_tab_id" property="blogTabId" jdbcType="INTEGER"/>        <result column="blog_title" property="blogTitle" jdbcType="VARCHAR"/>        <result column="blog_desc" property="blogDesc" jdbcType="VARCHAR"/>        <result column="blog_cover_img" property="blogCoverImg" jdbcType="VARCHAR"/>        <result column="blog_publish_time" property="blogPublishTime" jdbcType="TIMESTAMP"/>        <result column="blog_last_update_time" property="blogLastUpdateTime" jdbcType="TIMESTAMP"/>        <result column="blog_read_times" property="blogReadTimes" jdbcType="INTEGER"/>        <result column="blog_order" property="blogOrder" jdbcType="INTEGER"/>        <result column="blog_status" property="blogStatus" jdbcType="INTEGER"/>    </resultMap>            <select id="queryAllByUserId" resultMap="baseResultMap">        SELECT * FROM uc_blogs blog WHERE blog.blog_belong_to = #{userId} AND blog.blog_status = #{status} order  by blog_order asc    </select>    <select id="blogDetail" resultMap="baseResultMap"   >            SELECT * FROM uc_blogs blog WHERE blog.id = #{blogId} AND blog.blog_status = #{status}    </select>                                                    </mapper>