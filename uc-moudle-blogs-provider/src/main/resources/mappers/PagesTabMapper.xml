<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.uc.moudle.blogs.provider.mapper.PagesTabMapper">        <resultMap id="BaseResultMap" type="com.uc.blogs.api.entity.UcPagesTab">        <id column="id" property="id" jdbcType="INTEGER"/>        <result column="tab_create_time" property="tabCreateTime" jdbcType="TIMESTAMP"/>        <result column="tab_name" property="tabName" jdbcType="VARCHAR"/>        <result column="tab_level" property="tabLevel" jdbcType="INTEGER"/>        <result column="tab_parent_id" property="tabParentId" jdbcType="INTEGER"/>        <result column="tab_status" property="tabStatus" jdbcType="INTEGER"/>        <result column="tab_order" property="tabOrder" jdbcType="INTEGER"/>        <result column="tab_belong_to" property="tabBelongTo" jdbcType="INTEGER"/>        <result column="tab_url_sign" property="tabUrlSign" jdbcType="VARCHAR"/>    </resultMap>            <select id="queryUserTabs" resultMap="BaseResultMap">        SELECT * FROM uc_pages_tab tab WHERE tab.tab_belong_to = #{userId} AND tab.tab_status = #{status} order by tab_order asc;    </select>                                                    </mapper>